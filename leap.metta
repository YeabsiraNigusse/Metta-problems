(= (leap $y)
    (if (== (% $y 400) 0)
        (its a leap year)
        (if (not(== ( % $y 4) 0))
            (its not a leap year)
            (if (not(== ( % $y 100) 0))
                 (its a leap year)
                 (its not a leap year))))
)
!(leap 2023)
!(leap 2024)
!(leap 2025)
!(leap 2026)
!(leap 2027)
!(leap 2028)
